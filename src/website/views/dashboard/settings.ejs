<script>
    var time = 5; 
    var intervalID;
function redirect() {
    if (time > 0) { 
        time--; 
        document.getElementById("message").innerHTML = time;
    } else { 
        window.location.href = "<%= botconfig.domain %>/dashboard/<%= guild.id %>/server"; 
        clearInterval(intervalID);
    }
}

</script>
</head>

<body class="rgb2 mt-5" onload="intervalID = setInterval(redirect, 1000)">
    
