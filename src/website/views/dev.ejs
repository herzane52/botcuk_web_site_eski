				<script>
					// Get the canvas element and its context
					var canvas = document.getElementById("canvas");
					var ctx = canvas.getContext("2d");

					// Set the canvas size to the window size
					canvas.width = window.innerWidth;
					canvas.height = window.innerHeight;

					// Create an array of characters to use for the matrix rain
					var chars = "HERZANE".split(0);

					// Create an array of drops, each with a random x position and a negative y position
					var drops = [];
					for (var i = 0; i < canvas.width / 1; i++) {
						drops[i] = {x: i * 10, y: -Math.random() * canvas.height};
					}

					// Define the color and font of the text
					ctx.fillStyle = "green";
					ctx.font = "20px monospace";

					// Define a function to draw the matrix rain
					function draw() {
						// Clear the canvas with a semi-transparent black fill
						ctx.fillStyle = "rgba(0,0,0,0.05)";
						ctx.fillRect(0,0,canvas.width,canvas.height);

						// Loop through the drops array
						for (var i = 0; i < drops.length; i++) {
						// Get the current drop object
						var drop = drops[i];

						// Pick a random character from the chars array
						var char = chars[Math.floor(Math.random() * chars.length)];

						// Draw the character at the drop's x and y position
						ctx.fillStyle = "green";
						ctx.fillText(char, drop.x, drop.y);

						// Increase the drop's y position by a random amount
						drop.y += Math.random() * 5;

						// If the drop's y position is past the canvas height, reset it to a negative value
						if (drop.y > canvas.height) {
							drop.y = -Math.random() * canvas.height;
						}
						}
					}

					// Call the draw function every 30 milliseconds
					setInterval(draw,30);
				</script>
